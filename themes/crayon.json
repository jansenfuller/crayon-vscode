{
	"name": "Crayon VSCode",
	"type": "dark",
	"colors": {
		"errorForeground": "#B27B78",
		"editor.background": "#1e1e1e",
		"editor.foreground": "#d4d4d4",
		"editor.selectionBackground": "#8C9FB863",
		"editor.selectionHighlightBackground": "#8C9FB863",
		"editorLineNumber.foreground": "#586270",
		"activityBarBadge.background": "#5A7CA4",
		"activityBar.background": "#1e1e1e",
		"sideBarTitle.foreground": "#d4d4d4",
		"sideBarTitle.background": "#1e1e1e",
		"statusBar.background": "#5A7CA4",
		"statusBar.noFolderBackground": "#B59CD8",
		"statusBar.debuggingBackground": "#B27B78",
		"titleBar.activeBackground": "#2d2d2d",
		"gitDecoration.untrackedResourceForeground": "#99ae63"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#D8D8D8",
				"background": "#1E1E1E"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#586270"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#D8C27A"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.other",
				"keyword.control"
			],
			"settings": {
				"foreground": "#B39696"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#B39696"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"meta.tag",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#B39696"
			}
		},
		{
			"name": "Punctuation, Misc",
			"scope": [
				"punctuation",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"name": "Other Meta Tag",
			"scope": [
				"meta.tag.sgml",
				"meta.tag.sgml.html"
			],
			"settings": {
				"foreground": "#586270"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#8CA1B8"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#D8C27A"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#D9C891"
			}
		},
		{
			"name": "String, Symbols",
			"scope": [
				"string",
				"constant.other.symbol",
				"punctuation.definition.constant",
				"constant.other.key",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#9FAD78"
			}
		},
		{
			"name": "Inherited Class",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#FFF"
			}
		},
		{
			"name": "Importing",
			"scope": [
				"import",
				"keyword.other.special-method",
				"keyword.control.import",
				"keywrod.control.import.from",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#8C9FB8"
			}
		},
		{
			"name": "Class",
			"scope": [
				"meta.class",
				"meta.use.php"
			],
			"settings": {
				"foreground": "#8C9FB8"
			}
		},
		{
			"name": "Support",
			"scope": [
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#8C9FB8"
			}
		},
		{
			"name": "Exceptions",
			"scope": [
				"support.type.exception"
			],
			"settings": {
				"foreground": "#B5A3CF"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B27B78"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#D8C27A"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#D8C27A"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#B27B78"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#B27B78"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#383e47"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#586270"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#CC99B3"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#798494"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#8dc9c3"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"foreground": "#B39696",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#B27B78"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Markdown - Plain",
			"scope": [
				"text.html.markdown"
			],
			"settings": {
				"foreground": "#D8D8D8"
			}
		},
		{
			"name": "Markdown - Line Break",
			"scope": [
				"text.html.markdown meta.dummy.line-break"
			],
			"settings": {
				"foreground": ""
			}
		},
		{
			"name": "Markdown - Heading",
			"scope": [
				"markdown.heading",
				"markup.heading",
				"markup.heading.markdown "
			],
			"settings": {
				"foreground": "#FFFFFF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown - Heading Punctuation",
			"scope": [
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#B39696"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b59cd8"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d8c27a"
			}
		},
		{
			"name": "Markup - Bold-Italic",
			"scope": [
				"markup.bold markup.italic",
				"markup.italic markup.bold",
				"markup.quote markup.bold",
				"markup.bold markup.italic string",
				"markup.italic markup.bold string",
				"markup.quote markup.bold string"
			],
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#c48d62"
			}
		},
		{
			"name": "Markup - Underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#B27B78"
			}
		},
		{
			"name": "Markup - Strike",
			"scope": [
				"markup.strike"
			],
			"settings": {
				"fontStyle": "strike",
				"foreground": ""
			}
		},
		{
			"name": "Markdown - List",
			"scope": [
				"punctuation.definition.list_item.markdown",
				"beginning.punctuation.definition.list"
			],
			"settings": {
				"foreground": "#B39696"
			}
		},
		{
			"name": "Markdown - Blockquote",
			"scope": [
				"markup.quote",
				"markup.fenced_code",
				"markup.inline.raw.string"
			],
			"settings": {
				"foreground": "#d8c27a"
			}
		},
		{
			"name": "Markdown - Blockquote Punctuation",
			"scope": [
				"punctuation.definition.blockquote.markdown"
			],
			"settings": {
				"foreground": "#c48d62"
			}
		},
		{
			"name": "Markdown - Link, Description, Anchor",
			"scope": [
				"string.other.link",
				"constant.other.reference.link",
				"markup.underline"
			],
			"settings": {
				"foreground": "#B39696"
			}
		},
		{
			"name": "Markdown - Separator",
			"scope": [
				"meta.separator"
			],
			"settings": {
				"fontStyle": "bold",
				"background": "#00000050",
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markup - Table",
			"scope": [
				"markup.table"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}
